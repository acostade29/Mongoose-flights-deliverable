<%- include('../partials/header')  %>

<table> 
    <thead>
        <tr>
            <th>Flight</th>
            <th>Airline</th>
            <th>Airport</th>
            <th>Departure Date</th>
        </tr>
    </thead>
    <tbody>
        <% flights.forEach(function(f) { %>
            <tr>
                <td><%= f.flightNo %></td>
                <td><%= f.airline %></td>
                <td><%= f.Airport %></td>   
                <td><%= f.departs %></td>
                   
            </tr>
        <% }) %>
    </tbody>
</table>

<%- include('../partials/footer') %> 

